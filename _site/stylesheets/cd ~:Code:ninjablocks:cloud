cd ~/Code/ninjablocks/cloud
git checkout master
git checkout -b heroku-push
rm models
git add .
git commit -a -m "Pre-heroku push commit"
git subtree add -P models git@github.com:ninjablocks/shared-libs.git master
git push prod-webapp +heroku-push:master
rm -rf models
ln -s ../shared-libs models
git add .
git commit -a -m "Post-heroku push commit"
git checkout develop
git merge heroku-push
git branch -D heroku-push
